{
  "manifest_version": 3,
  "name": "Rumble Timestamp Jump",
  "description": "Makes Rumble comment timestamps clickable to jump to video time.",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "logos/rumble-logo-16.png",
      "32": "logos/rumble-logo-32.png",
      "48": "logos/rumble-logo-48.png",
      "128": "logos/rumble-logo-128.png"
    },
    "default_title": "Jump to timestamp"
  },
  "content_scripts": [
    {
      "matches": ["*://rumble.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "logos/rumble-logo-128.png"
  },
  "permissions": []
}
